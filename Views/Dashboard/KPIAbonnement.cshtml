                <div>
                    <div id="chartContainer" style="height: 370px; width: 100%;"></div>
                </div>
            </div>

        </div>
    </div>
    <div class="row">
            <div class="col-lg-6">
                <div class="ibox float-e-margins">
                    <div class="ibox-title">
                        <h5>Radar Chart Example</h5>
                    </div>
                    <div class="ibox-content">
                        <div>
                            <canvas id="radarChart" style="height: 370px; width: 100%;"></canvas>
                          
                        </div>
                    </div>
                </div>
            </div>
        <div class="col-lg-6">
            <div class="ibox float-e-margins">
                <div class="ibox-content">
                    <figure class="highcharts-figure">
                        <div id="containerr"></div>
                        <p class="highcharts-description">

                        </p>
                    </figure>
                </div>
            </div>
        </div>
    </div>

    <div class="col-lg-12">
        <div class="clients-list">
            <ul class="nav nav-tabs">

                @foreach (var item in ViewBag.demandeParAgenceAnnee)
                {
                    <li class="">
                        <a data-toggle="tab" href="#Annee_@item.GetType().GetProperty("Key").GetValue(item) ">
                            <i class="fa fa-calendar"></i>@item.GetType().GetProperty("Key").GetValue(item)
                        </a>
                    </li>
                }

            </ul>
            <div class="tab-content">

                @foreach (var grouped in ViewBag.demandeParAgenceAnnee)
                {
                    if (grouped.GetType().GetProperty("Key").GetValue(grouped) == 2023)
                    {
                        <div id="Annee_@grouped.GetType().GetProperty("Key").GetValue(grouped)"
                             class="tab-pane active text-black-50">
                            <div class="full-height-scroll">
                                <div class="table-responsive">
                                    <table class="table table-striped table-hover datatable-style">
                                        <thead>
                                            <tr>

                                                <th>Région</th>
                                                <th>Agence</th>
                                                <th>Total Demandes</th>
                                                <th>Total Demandes Annulée</th>
                                                <th>Total Demandes Valides</th>

                                            </tr>
                                        </thead>
                                        <tbody>
                                            @foreach (var item in grouped)
                                            {
                                            <tr>

                                                <td>@item.CodeRegion</td>
                                                <td>@item.Agence</td>
                                                <td>@item.Count</td>
                                                <td>@item.DemandeAnnulees</td>
                                                <td>@item.DemandeValide</td>
                                            </tr>
                                            }
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>

                    }
                    else
                    {

                        <div id="Annee_@grouped.GetType().GetProperty("Key").GetValue(grouped)"
                             class="tab-pane text-black-50">
                            <div class="full-height-scroll">
                                <div class="table-responsive">
                                    <table class="table table-striped table-hover datatable-style">
                                        <thead>
                                            <tr>

                                                <th>Région</th>
                                                <th>Agence</th>
                                                <th>Total Demandes</th>
                                                <th>Total Demandes Annulée</th>
                                                <th>Total Demandes Valides</th>

                                            </tr>
                                        </thead>
                                        <tbody>
                                            @foreach (var item in grouped)
                                            {
                                            <tr>

                                                <td>@item.CodeRegion</td>
                                                <td>@item.Agence</td>
                                                <td>@item.Count</td>
                                                <td>@item.DemandeAnnulees</td>
                                                <td>@item.DemandeValide</td>
                                            </tr>
                                            }
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>

                    }
                }
            </div>
        </div>
    </div>
</div>

