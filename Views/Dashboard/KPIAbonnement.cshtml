                        <h3 class="font-bold no-margins">
                            Abonnement <strong> Suspendus </strong> le
                        </h3>

                        <small>@System.DateTime.Now.ToString("dd/MM/yyyy")</small>
                </div>

            </div>

            <div class="col-xs-3">

                <div class="widget white-bg btn-outline btn-warning style1 text-center  h-150" style="border:dotted;  border-color:#FEC163;">

                    <i class="fa fa-address-card fa-4x m-b-sm"></i>
                    <h3 class="m-xs">@ViewBag.AbonnementSuspendus</h3>
                    <h3 class="font-bold no-margins">
                        Abonnement <strong> Suspendus </strong> @ViewBag.year
                    </h3>
                    <small>@System.DateTime.Now.ToString("dd/MM/yyyy")</small>

                </div>

            </div>



        </div>

