                    <h3 class="m-xs">@ViewBag.AbonnementMigre</h3>
                    <h3 class="font-bold no-margins">
                        Abonnements <strong> Migrés </strong> Année courante
                    </h3>
                    <small>@System.DateTime.Now.ToString("dd/MM/yyyy")</small>
                </div>

            </div>
        </div>
