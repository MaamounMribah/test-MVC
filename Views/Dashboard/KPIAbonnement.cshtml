                        <h3 class="m-xs">@ViewBag.AbonnementResilieToday</h3>
                        <h3 class="font-bold no-margins">
                            Abonnement <strong> Résiliés </strong>  le
                        </h3>
                        <small>@System.DateTime.Now.ToString("dd/MM/yyyy")</small>
                </div>

            </div>

