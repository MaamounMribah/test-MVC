@model BeeProj.Models.AgenceModel
@{
    ViewBag.Title = "SupprimerAgence";
    Layout = "~/Views/Shared/_Layout.cshtml";
}




<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-lg-10">
        <h2>Décalage Facture</h2>
        <ol class="breadcrumb">
            <li>
                <a href="@Url.Action("index","Administration")">Administration </a>
            </li>
            <li>
                <a href="@Url.Action("index","Correction")">Correction </a>
            </li>
            <li class="active">
                <strong> <a href="@Url.Action("SupprimerAgence","Correction")">Supprimer Agence</a></strong>
            </li>
        </ol>
    </div>

</div>
<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">

            @if (ViewBag.pasdereglement != null)
            {
                <div class="alert alert-danger alert-dismissable">
                    <button aria-hidden="true" data-dismiss="alert" class="close" type="button">×</button>
                    Aucun réglement existe avec les information saisies !
                </div>
            }


            <div class="ibox float-e-margins">
                <div class="ibox-title" style="height:80px;">

                    @using (Html.BeginForm("SupprimerAgence", "Correction", FormMethod.Post))
                    {

                        <div class="col-sm-6">
                            <label class="control-label">Recherche par numero du facture  </label>
                            <input type="text" class="form-control" id="codeagence" name="codeagence" value="@ViewBag.pasdereglement" />
                        </div>
                        <div class="col-sm-2">
                            <br />
                            <button type="submit" class="btn btn-purplebee btn-rounded btn-outline" id="showClientInfos">Rechercher  </button>

                        </div>
                        <br /><br />


                    }


                </div>
            </div>
        </div>
</div>
</div>