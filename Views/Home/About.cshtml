@{
    ViewBag.Title = "About";
    Layout = "~/Views/Shared/_Layout.cshtml";

}



    <style type="text/css">
        html {
            box-sizing: border-box;
        }

        *, *:before, *:after {
            box-sizing: inherit;
        }

        body {
            font-family: 'Droid Sans', sans-serif;
            background: #f2f2f2;
            font-size: 14px;
            line-height: 21px;
        }

        .container {
            width: 960px;
            margin: 20px auto;
        }

        @@media only screen and (min-width: 768px) and (max-width: 1000px) {
            .container {
                width: 768px;
            }
        }

        @@media only screen and (max-width: 767px) {
            .container {
                width: 420px;
            }
        }

        @@media only screen and (max-width: 480px) {
            .container {
                width: 300px;
            }
        }

        a img {
            border: none;
        }

        h1, h2, h3, h4, h5, h6 {
            font-weight: normal;
        }

        h1 {
            font-size: 26px;
            line-height: 32px;
        }

        p, ul {
            margin-bottom: 10px;
        }
    </style>
    <!-- End Demo scripts -->
    <!-- Jquery is required, embed on your page if not already - don't embed 2 versions -->




<script src="~/js/jquery-3.1.1.min.js"></script>
    <!-- End Jquery -->
    <!-- Map scripts - add the below to your page -->
    <!-- jsmaps-panzoom.js is optional if you are using enablePanZoom -->
    <link href="~/js/jsmaps/jsmaps.css" rel="stylesheet" type="text/css" />
    <script src="~/js/jsmaps/jsmaps-libs.js" type="text/javascript"></script>
    <script src="~/js/jsmaps/jsmaps-panzoom.js"></script>
    <script src="~/js/jsmaps/jsmaps.min.js" type="text/javascript"></script>
    <script src="~/js/maps/tunisia.js" type="text/javascript"></script>
    <!-- End Map scripts -->


    <div class="container">

        <!-- Map html - add the below to your page -->
        <div class="jsmaps-wrapper" id="tunisia-map"></div>
        <!-- End Map html -->
    </div>

    <script type="text/javascript">

    $(function() {

      $('#tunisia-map').JSMaps({
        map: 'tunisia'
      });

    });

    </script>
