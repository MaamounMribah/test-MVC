<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-lg-9">
        <h2>EASYBEEExport</h2>
        <ol class="breadcrumb">
            <li>
                <a href="#">Acceuil</a>
            </li>
            <li class="active">
                <strong>Export Excel</strong>
            </li>
        </ol>
    </div>
</div>
<div class="wrapper wrapper-content  animated fadeInRight">
    <div class="ibox-content">
        <div class="row">
            <div class="col-lg-12">
                <div class="row">

                    <div class="col-md-6" id="listingfacture">
                        <div class="widget bg-purplebee text-center">
                            <i class="fa fa-file-o fa-3x"></i>
                            <input class="btn btn-purplebee m-xs" type="submit" value="Listing factures" id="btlistingfacture" data-toggle="modal" data-target="#Export2" @*onClick="ClickedMe()"*@ />
                        </div>
                    </div>

                    <div class="col-md-6" id="reglement">
                        <div class="widget bg-purplebee text-center">
                            <i class="glyphicon glyphicon-list fa-3x"></i>
                            <input class="btn btn-purplebee m-xs" type="submit" value="Liste des réglements clients" id="btreglement" data-toggle="modal" data-target="#Export1" @*onClick="ClickedMebtreglement()"*@ />
                        </div>
                    </div>

                    <div class="col-md-6" id="listeabonnement">
                        <div class="widget bg-purplebee  text-center">
                            <i class="fa fa-address-card-o fa-3x"></i>
                            <input class="btn btn-purplebee m-xs" type="submit" value="Liste des abonnements" id="btlisteabonnement" data-toggle="modal" data-target="#Export3" onClick="ClickedMebtlisteabonnement()" />
                        </div>
                    </div>

                    <div class="col-md-6" id="listeabomigres">
                        <div class="widget bg-purplebee  text-center">
                            <i class="fa fa-file-archive-o fa-3x"></i>
                            <input class="btn btn-purplebee m-xs" type="submit" value="Liste des abonnements migrés" id="btlisteabomigres" data-toggle="modal" data-target="#Export4" onClick="ClickedMebtlisteabomigres()" />
                        </div>
                    </div>
                </div>
                <div class="row">

                    <div class="col-md-6" id="listeavance">
                        <div class="widget bg-purplebee text-center">
                            <i class="fa fa-clipboard fa-3x"></i>
                            <input class="btn btn-purplebee m-xs" type="submit" value="Liste des reglements(Avances Ou Contre Recu)" id="btlistereg" data-toggle="modal" data-target="#Export9" onClick="ClickedMebtlisteavance()" />
                        </div>
                    </div>
                    <div class="col-md-6" id="listeavancerecu">
                        <div class="widget bg-purplebee text-center">
                            <i class="fa fa-clipboard fa-3x"></i>
                            <input class="btn btn-purplebee m-xs" type="submit" value="Liste avance ou contre recu" id="btlisteavance" data-toggle="modal" data-target="#Export12" onClick="ClickedMebtlisteavance()" />
                        </div>
                    </div>



                    <div class="col-md-6" id="listenonpayes">
                        <div class="widget bg-purplebee  text-center">
                            <i class="fa fa-file fa-3x"></i>
                            <input class="btn btn-purplebee m-xs" type="submit" value="Liste facture non payees" id="btlistenonpayes" data-toggle="modal" data-target="#Export7" onClick="ClickedMebtlistenonpayes()" />
                        </div>
                    </div>
                    <div class="col-md-6" id="listepayees">
                        <div class="widget bg-purplebee  text-center">
                            <i class="fa fa-file-text fa-3x"></i>
                            <input class="btn btn-purplebee m-xs" type="submit" value="Liste facture payees" id="btlistepayees" data-toggle="modal" data-target="#Export8" onClick="ClickedMebtlistepayees()" />
                        </div>
                    </div>


                </div>
                <div class="row">
                    <div class="col-md-6" id="ListeRemarques">
                        <div class="widget bg-purplebee  text-center">
                            <i class="fa fa-paper-plane fa-3x"></i>
                            <input class="btn btn-purplebee m-xs" type="submit" value="Liste Remarques" id="btListeRemarques" data-toggle="modal" data-target="#Export10" />
                        </div>

                    </div>
                    <div class="col-md-6" id="listeabonnementxl">
                        <div class="widget bg-purplebee  text-center">
                            <i class="fa fa-address-card-o fa-3x"></i>
                            <input class="btn btn-purplebee m-xs" type="submit" value="Liste des abonnements .xlsx" id="btlisteabonnement" data-toggle="modal" data-target="#Export13" onClick="ClickedMebtlisteabonnement()" />
                        </div>
                    </div>
                    <div class="col-md-6" id="ExportFactureVente">
                        <div class="widget bg-purplebee  text-center">
                            <i class="fa fa-list-alt fa-3x"></i>
                            <input class="btn btn-purplebee m-xs" type="submit" value="Export des factures de Ventes" id="btExportFactureVente" data-toggle="modal" data-target="#Export11" />
                        </div>

                    </div>
                    <div class="col-md-6" id="listearticle">
                        <div class="widget bg-purplebee  text-center">
                            <i class="fa fa-book fa-3x"></i>
                            <input class="btn btn-purplebee m-xs" type="submit" value="Liste des articles reservés" id="btlistearticle" data-toggle="modal" data-target="#Export5" onClick="ClickedMebtsoldeclient()" />
                        </div>
                    </div>
                    <div class="col-md-6" id="ListeStock">
                        <div class="widget bg-purplebee  text-center">
                            <i class="fa fa-server fa-3x"></i>
                            <input class="btn btn-purplebee m-xs" type="submit" value="Liste Stock par dépot" id="btListeStock" data-toggle="modal" data-target="#Export6" onClick="ClickedMebtListeStock()" />
                        </div>

                    </div>

                    <div class="col-md-6" id="ListeStock">
                        <div class="widget bg-purplebee  text-center">
                            <i class="fa fa-server fa-3x"></i>
                            <input class="btn btn-purplebee m-xs" type="submit" value="Liste Stock total" id="btListeStock" data-toggle="modal" data-target="#ExportSto" onClick="ClickedMebtListeStock()" />
                        </div>

                    </div>


                </div>
                <div class="row">

                    <div class="col-md-6" id="listeavance">
                        <div class="widget bg-purplebee text-center">
                            <i class="fa fa-clipboard fa-3x"></i>
                            <input class="btn btn-purplebee m-xs" type="submit" value="Listing factures DAF" id="btlistereg" data-toggle="modal" data-target="#ExportDaf" onClick="ClickedMebtlisteavance()" />
                        </div>
                    </div>
                  

                </div>
            </div>
        </div>
    </div>

</div>


<div class="modal inmodal" id="Export" tabindex="-1" role="dialog" aria-hidden="true" data-keyboard="false" data-backdrop="static">
    <div class="modal-dialog">
        <div class="modal-content animated flipInY">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Ok</span></button>
                <h4 class="modal-title text-purplebee">Export Excel</h4>
            </div>
            <div class="modal-body">
                <div class="row">
                    <form role="form">
                        <div class="form-group">
                            @if (ViewBag.msg == null)
                            {
                                <center>
                                    <div class="spinner-border text-purplebee" role="status">
                                        <i class="fa fa-spinner fa-spin" style="font-size:50px"></i>
                                        <br />
                                        <h1>Veuillez patienterjusqu'à la fin d'export...</h1>
                                    </div>
                                    <br />
                                    <h5 class="text-purplebee">  Une fois l'export faite avec succés , merci d'actualiser la page afin de continuer les autres exports</h5>
                                    <br />
                                </center>
                            }
                            else
                            {
                                <i class="fa fa-check-circle"></i>
                                <br />
                                <h3 class="bg-danger">Merci,vous pouvez continuer les autres exports maintenant </h3>
                            }


                        </div>
                    </form>
                </div>
            </div>

        </div>
    </div>
</div>

