
@{
    ViewBag.Title = "ListeDemandesTransfert";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
    .dataTables_filter, .dataTables_info, .dataTables_length {
        display: none;
    }
</style>
<link href="~/css/plugins/dataTables/datatables.min.css" rel="stylesheet">
<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-lg-10">
        <h2>Liste des bons de Sortie </h2>
        <ol class="breadcrumb">
            <li>
                <a href="@Url.Action("index","Home")">Acceuil </a>

            </li>
            <li>
                <a href="@Url.Action("ListeBonDeSortie","SAV")">SAV </a>

            </li>
            <li class="active">
                <strong>Liste des bons de Sortie</strong>
            </li>
        </ol>
    </div>
    <div class="col-lg-2">

    </div>
</div>





<div class="wrapper wrapper-content animated fadeInRight">

    <div class="row">
        <div class="col-lg-12">
            <div class="wrapper wrapper-content animated fadeInUp">

                <div class="ibox">
                    <div class="ibox-title">
                        
                            <div class="ibox-tools">
                                <a href=@Url.Action("BonDeSortie", "SAV") class="btn btn-primary btn-xs">Nouvelle demande</a>
                            </div>
                       


                        <br />




                    </div>
                    <div class="ibox-content">
                        <div class="project-list">
                            <table class="table table-hover dataTables-example" id="DemandeTR">

                                <thead>

                                    <tr>

                                      

                                        <th>

                                            Référence
                                        </th>

                                        <th>Date</th>

                                        <th>
                                            Depot 
                                        </th>
                                        <th>
                                            Abonnement
                                        </th>
                                        

                                        <th>

                                          Client
                                        </th>



                                    </tr>
                                </thead>

                            </table>


                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>

</div>
<script src="~/js/jquery-3.1.1.min.js"></script>

<script src="~/js/plugins/metisMenu/jquery.metisMenu.js"></script>



<script src="~/js/plugins/dataTables/datatables.min.js"></script>


<script>
                        $(document).ready(function () {

                            var table = $("#DemandeTR").DataTable({
                                "ajax": {

                                    "url": "/SAV/getlistedesBondeSortie/",
                                    "type": 'get',
                                    "datatype": "json",
                                    "ordering": true





                                },
                                "columns": [
                                   { "data": "piece" },
                                   { "data": "date" },
                                   { "data": "depot" },
                                   { "data": "abonnement" },
                                   { "data": "client" },
                                   
                                ],




                            });







                        });


</script>
